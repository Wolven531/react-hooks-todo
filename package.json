	"name": "react-hooks-todo",
	"version": "0.1.1",
	"private": true,
	"dependencies": {
		"bfj": "6.1.1",
		"dotenv": "6.0.0",
		"dotenv-expand": "4.2.0",
		"fs-extra": "7.0.1",
		"identity-obj-proxy": "3.0.0",
		"moment": "^2.24.0",
		"react": "^16.8.6",
		"react-app-polyfill": "^0.2.2",
		"react-dom": "^16.8.6",
		"resolve": "1.10.0",
		"typescript": "3.4.2",
		"uuid": "^3.3.2"
	},
	"scripts": {
		"start": "node scripts/start.js",
		"build": "node scripts/build.js",
		"test": "node scripts/test.js --coverage --watchAll"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/core": "7.2.2",
		"@svgr/webpack": "4.1.0",
		"@types/cheerio": "^0.22.11",
		"@types/enzyme": "3.9.0",
		"@types/enzyme-adapter-react-16": "^1.0.5",
		"@types/jasmine": "^3.3.12",
		"@types/mocha": "^5.2.6",
		"@types/node": "11.13.2",
		"@types/react": "16.8.13",
		"@types/react-dom": "16.8.3",
		"@types/uuid": "^3.4.4",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "9.0.0",
		"babel-jest": "^24.8.0",
		"babel-loader": "8.0.5",
		"babel-plugin-named-asset-import": "^0.3.1",
		"babel-preset-react-app": "^7.0.2",
		"case-sensitive-paths-webpack-plugin": "2.2.0",
		"cheerio": "^1.0.0-rc.3",
		"css-loader": "1.0.0",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.12.1",
		"eslint": "5.12.0",
		"eslint-config-react-app": "^3.0.8",
		"eslint-loader": "2.1.1",
		"eslint-plugin-flowtype": "2.50.1",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-jsx-a11y": "6.1.2",
		"eslint-plugin-react": "7.12.4",
		"file-loader": "2.0.0",
		"html-webpack-plugin": "4.0.0-alpha.2",
		"jest": "^24.8.0",
		"jest-localstorage-mock": "^2.4.0",
		"jest-pnp-resolver": "^1.2.1",
		"jest-resolve": "^24.8.0",
		"jest-watch-typeahead": "^0.3.1",
		"mini-css-extract-plugin": "0.5.0",
		"mocha": "^6.1.4",
		"mock-local-storage": "^1.1.8",
		"optimize-css-assets-webpack-plugin": "5.0.1",
		"pnp-webpack-plugin": "1.2.1",
		"postcss-flexbugs-fixes": "4.1.0",
		"postcss-loader": "3.0.0",
		"postcss-preset-env": "6.5.0",
		"postcss-safe-parser": "4.0.1",
		"react-dev-utils": "^8.0.0",
		"sass-loader": "7.1.0",
		"style-loader": "0.23.1",
		"terser-webpack-plugin": "1.2.2",
		"url-loader": "1.1.2",
		"webpack": "4.28.3",
		"webpack-dev-server": "3.1.14",
		"webpack-manifest-plugin": "2.0.4",
		"workbox-webpack-plugin": "3.6.3"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"resolver": "jest-pnp-resolver",
		"setupFiles": [
			"react-app-polyfill/jsdom",
			"jest-localstorage-mock"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/components/**/**.test.{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jsdom",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		],
		"watchPlugins": [
			"/repos/react-hooks-todo/node_modules/jest-watch-typeahead/filename.js",
			"/repos/react-hooks-todo/node_modules/jest-watch-typeahead/testname.js"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	}
}
